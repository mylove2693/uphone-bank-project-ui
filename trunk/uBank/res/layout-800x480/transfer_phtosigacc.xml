<?xml version="1.0" encoding="utf-8"?>
	<!--
手机到签约账户转账的
-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="match_parent"
	android:orientation="vertical">
	<!--
  请输入转账金额
  -->
	<include android:id="@+id/transfer_amt_txt" layout="@xml/text_view_18"
		android:layout_marginLeft="20dip" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<!--
		转账金额输入框
	-->
	<include android:id="@+id/transfer_amt_edit" layout="@xml/et_amt"
		android:layout_gravity="center_horizontal" android:layout_width="280dip"
		android:layout_height="wrap_content" />
	<!--
		白线
	-->
	<include layout="@xml/white_line" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />


	<!--
  请输入转账账户
  -->
	<include android:id="@+id/transfer_acc_txt" layout="@xml/text_view_18"
		android:layout_marginLeft="20dip" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<!--
		转账账户输入框
	-->
	<include android:id="@+id/transfer_acc_edit" layout="@xml/et_acc"
		android:layout_gravity="center_horizontal" android:layout_width="280dip"
		android:layout_height="wrap_content" />
	<!--
		白线
	-->
	<include layout="@xml/white_line" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />


	<!--
  请输入账户密码
  -->
	<include android:id="@+id/transfer_psd_txt" layout="@xml/text_view_18"
		android:layout_marginLeft="20dip" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<!--
		账户密码输入框
	-->
	<include android:id="@+id/transfer_psd_edit" layout="@xml/et_psd"
		android:layout_gravity="center_horizontal" android:layout_width="280dip"
		android:layout_height="wrap_content" />
	<!--
		白线
	-->
	<include layout="@xml/white_line" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />


	<include android:id="@+id/next_btn" layout="@xml/button"
	android:layout_marginTop="20dip"
		android:layout_width="wrap_content" android:layout_gravity="center_horizontal"
		android:layout_height="wrap_content" />
</LinearLayout>
