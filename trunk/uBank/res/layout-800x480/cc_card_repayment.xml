<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical">

	<ExpandableListView android:id="@+id/expandList"
		android:layout_width="fill_parent" android:layout_height="wrap_content" />
	<include layout="@xml/white_line" android:layout_width="fill_parent"
		android:layout_height="1px" />
	<!-- 其他信用卡还款 -->
	<include android:id="@+id/cc_tv_otherCC" layout="@xml/text_view_18"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_marginLeft="50px" />
	<include android:id="@+id/cc_tv_cc_num" layout="@xml/text_view_18"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_marginLeft="50dip" />
	<include android:id="@+id/cc_et_cc" layout="@xml/et_acc"
		android:layout_width="250dip" android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal" />
	<include android:id="@+id/cc_btn_next" layout="@xml/button"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal" android:layout_marginTop="10dip" />


</LinearLayout>
