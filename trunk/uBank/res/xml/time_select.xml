<?xml version="1.0" encoding="utf-8"?>
	<!--
时间选择组件
-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="match_parent">

	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<!--
从：
		-->
		<include android:id="@+id/from_text" layout="@xml/text_view_18"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_marginLeft="25dip" />
		<!--
开始时间
		-->
		<Button android:id="@+id/Start_Time" android:layout_width="wrap_content"
			android:layout_height="35dip" />

	</LinearLayout>

	<LinearLayout android:orientation="horizontal"
		android:layout_marginTop="25dip" android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<!--
到：
		-->
		<include android:id="@+id/to_text" layout="@xml/text_view_18"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<!--
结束时间
		-->
		<Button android:id="@+id/End_Time" android:layout_width="wrap_content"
			android:layout_height="35dip" />
	</LinearLayout>
	<!--
	查询按钮
	-->
	<include android:id="@+id/query" layout="@xml/button"
		android:layout_marginTop="25dip" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_gravity="center_horizontal" />

</LinearLayout>
