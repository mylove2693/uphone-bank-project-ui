<?xml version="1.0" encoding="utf-8"?>
<!--
账户选择的组合控件
-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="match_parent"
	android:orientation="vertical">
	<include layout="@layout/textview" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<include layout="@layout/white_line" android:layout_width="fill_parent"
		android:layout_height="2dip" />
	<include android:id="@+id/spinnerAccTyp" layout="@layout/spinner"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />
	<include android:id="@+id/AccTypText" layout="@layout/textview"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />
    <include layout="@layout/textview" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />
	<include android:id="@+id/spinnerAccNum" layout="@layout/spinner"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />
</LinearLayout>
