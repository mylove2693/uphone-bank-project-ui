<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<bank.view.common.MyButton
	android:id="@+id/myButton"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	/>
<bank.view.common.MyImageButton
	android:id="@+id/myImageButton"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:background="#00FFFFFF"
	android:src="@drawable/icon"
	/>
	<portfolio_control.Account_Select
	android:id="@+id/My_Account_Select"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	/>
	<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	>
	<include android:id="@+id/btnbu" layout="@layout/button"
	
	android:layout_width="wrap_content"
     android:layout_height="wrap_content"
	
android:layout_alignParentTop="true"
android:layout_alignParentLeft="true"

	/>
	<include android:id="@+id/nn" 
	layout="@layout/button"
	android:layout_below="@id/btnbu"
	android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="70dip"
	/>
	</RelativeLayout>
</LinearLayout>
