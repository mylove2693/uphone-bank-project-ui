<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout android:id="@+id/ff" android:layout_width="match_parent"
		android:layout_height="match_parent" android:orientation="horizontal">
		<include android:id="@+id/Credit_Name" layout="@xml/text_view_18"
			android:layout_width="wrap_content" android:layout_gravity="center_vertical"
			android:layout_height="wrap_content" />

		<include android:id="@+id/Credit_Name_Edit" layout="@xml/et_user"
			android:layout_width="wrap_content" android:layout_gravity="center_vertical"
			android:layout_height="wrap_content" />
	</LinearLayout>


	<LinearLayout android:id="@+id/f1" android:layout_width="match_parent"
		android:layout_height="match_parent" android:orientation="horizontal">

		<include android:id="@+id/Credit_Num" layout="@xml/text_view_18"
			android:layout_width="wrap_content" android:layout_gravity="center_vertical"
			android:layout_height="wrap_content" />

		<include android:id="@+id/Credit_Num_Edit" layout="@xml/et_acc"
			android:layout_width="wrap_content" android:layout_gravity="center_vertical"
			android:layout_height="wrap_content" />
	</LinearLayout>


	<LinearLayout android:layout_width="match_parent"
		android:layout_height="match_parent" android:orientation="horizontal">
		<include android:id="@+id/Zhengjian_Name" layout="@xml/text_view_18"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<include android:id="@+id/Zhengjian" layout="@xml/small_spinner"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
	</LinearLayout>
	<LinearLayout android:layout_width="match_parent"
		android:layout_height="match_parent" android:orientation="horizontal">
		<include android:id="@+id/Credit_psd" layout="@xml/text_view_18"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<include android:id="@+id/Credit_psd_Edit" layout="@xml/et_psd"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
	</LinearLayout>
</LinearLayout>
